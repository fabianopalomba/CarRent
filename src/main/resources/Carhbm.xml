<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.rentfegh.model.Car" table="cars" schema="feghrent">
        <id name="cars_id">
            <column name="cars_id" sql-type="int"/>
        </id>
        <property name="brand">
            <column name="brand" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="model">
            <column name="model" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>