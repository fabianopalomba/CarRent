<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.rentfegh.model.Rent" table="rent" schema="feghrent">
        <composite-id name="primaryKey" class="com.rentfegh.model.RentPK">
            <key-property name="user" type="text" column="email" />
            <key-property name="car" type="int" column="id" />
            <key-property name="initDate">
                <column name="initDate" sql-type="date" not-null="true"/>
            </key-property>
        </composite-id>
        <component name="user">
            <many-to-one name="email" class="com.rentfegh.model.User" insert="false" update="false">
                <column name="email" not-null="true" sql-type="text" length="45"/>
            </many-to-one>
        </component>
        <component name="car">
            <many-to-one name="carsid" class="com.rentfegh.model.Car" insert="false" update="false">
                <column name="id" not-null="true" sql-type="int"/>
            </many-to-one>
        </component>
        <property name="finDate">
            <column name="finDate" sql-type="date" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>